{
  "name": "node-starter-kit",
  "version": "1.0.0",
  "description": "serves as a starter kit for you node server applications",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "lint": "eslint **/*.js",
    "lint:fix": "prettier-eslint **/**/*.{js,json} --write",
    "prettier": "prettier **/**/*.{js,json} --write",
    "dev": "nodemon --exec babel-node src/server.js",
    "build": "babel src --copy-files --out-dir build",
    "test": "nyc --reporter=html --reporter=text mocha \"src/**/*.spec.js\" --require @babel/register --timeout 20000  --recursive --exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caleb-train/node-starter-kit.git"
  },
  "keywords": [
    "sales"
  ],
  "nyc": {
    "include": "src",
    "exclude": "**/*.spec.js"
  },
  "husky": {},
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "npm run prettier",
      "git add"
    ]
  },
  "author": "caleb",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/caleb-train/node-starter-kit/issues"
  },
  "homepage": "https://github.com/caleb-train/node-starter-kit#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.2",
    "dotenv": "^8.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.10",
    "express-session": "^1.16.2",
    "husky": "^3.0.8",
    "lint-staged": "^8.1.5",
    "mocha": "^6.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nock": "^11.3.5",
    "nodemon": "^1.19.3",
    "nyc": "^14.1.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "express-handlebars": "^5.2.0",
    "jsonwebtoken": "^8.5.1"
  }
}
